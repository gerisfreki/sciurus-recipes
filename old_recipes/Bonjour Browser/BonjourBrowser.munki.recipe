<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest Bonjour Browser disk image and imports into Munki.</string>
    <key>Identifier</key>
    <string>lan.sciurus.autopkg.munki.BonjourBrowser</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>BonjourBrowser</string>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>Tools/Networking/%NAME%</string>
        <key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
            <key>description</key>
            <string>Bonjour Browser is a utility that displays all the bonjour services on your local network.</string>
            <key>display_name</key>
            <string>Bonjour Browser</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>unattended_install</key>
            <true/>
			<key>category</key>
			<string>Tools</string>
			<key>developer</key>
			<string>Tildesoft</string>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.6.1</string>
    <key>ParentRecipe</key>
    <string>lan.sciurus.autopkg.download.BonjourBrowser</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%pathname%</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
        </dict>
    </array>
</dict>
</plist>
